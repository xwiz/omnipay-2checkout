{
    "name": "xwiz/omnipay-2checkout",
    "description": "2Checkout driver for the Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "2checkout"
    ],
    "homepage": "https://github.com/xwiz/omnipay-2checkout",
    "license": "MIT",
    "authors": [
        {
            "name": "Agbonghama Collins",
            "email": "me@w3guy.com",
            "homepage": "http://w3guy.com"
        },
        {
            "name": "Chibueze Opata",
            "email": "noemail@github.com",
            "homepage": "http://github.com/xwiz"
        }
    ],
    "require": {
        "omnipay/common": "^3.2",
      "ext-json": "*"
    },
    "require-dev": {
        "omnipay/tests": "^3.2",
        "satooshi/php-coveralls": "^0.7.1"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\TwoCheckoutPlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
